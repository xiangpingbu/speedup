<VariableConfiguration>
    <Model>XYBModel</Model>
    <Variables>
        <Variable name="CreditQueryTimes" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="creditQueryTimes"/>
            <property name="paramType" value="numerical"/>
            <property name="paramValue" value="(0-max)"/>
            <property name="description" value="return one's credit query time in three month"/>
            <property name="returnType" value="Integer"/>
        </Variable>

        <Variable name="CreditAmountSumVariable" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="creditLimit"/>
            <property name="paramType" value="numerical"/>
            <property name="paramValue" value="(0-max)"/>
            <property name="description" value="return one's total credit limit"/>
            <property name="returnType" value="Double"/>
        </Variable>

        <Variable name="CreditUtilizationVariable" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="creditUtilization"/>
            <property name="paramType" value="numerical"/>
            <property name="paramValue" value="(0-max)"/>
            <property name="description" value="return one's credit usage rate"/>
            <property name="returnType" value="Double"/>
        </Variable>

        <Variable name="PersonalEducation" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="personalEducation"/>
            <property name="paramType" value="categorical"/>
            <property name="paramValue" value="1|2|3|4"/>
            <property name="paramMapping" value="masterOrAbove|undergraduate|junior|others"/>
            <property name="description" value="return one's education"/>
            <property name="returnType" value="String"/>
        </Variable>

        <Variable name="PersonalLiveCase" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="personalLiveCase"/>
            <property name="paramType" value="categorical"/>
            <property name="paramValue" value="1|2|3|4|5|6|7|8"/>
            <property name="paramMapping" value="commercialMortgageRoom|noMortgageRoom|fundMortgageRoom|selfBuildingRoom|rentalRoom|Relatives|dormitory|others"/>
            <property name="description" value="return one's credit query time in three month"/>
            <property name="returnType" value="String"/>
        </Variable>

        <Variable name="ClientGender" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="clientGender"/>
            <property name="paramType" value="categorical"/>
            <property name="paramValue" value="1|2"/>
            <property name="paramMapping" value="male|female"/>
            <property name="description" value="return one's gender"/>
            <property name="returnType" value="String"/>
        </Variable>


        <Variable name="PersonalLiveJoin" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="personalLiveJoin"/>
            <property name="paramType" value="categorical"/>
            <property name="paramValue" value="1,|2,|1,2,|4,|3,|2,4,|1,4,|1,2,4,"/>
            <property name="paramMapping" value="parents|spouse&amp;child|parents&amp;spouse&amp;child|others|friend|spouse&amp;child&amp;others|parents&amp;others|parents&amp;spouse&amp;child&amp;others"/>
            <property name="description" value="return one's personal live join"/>
            <property name="returnType" value="String"/>
        </Variable>

        <Variable name="PersonalYearIncome" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="personalYearIncome"/>
            <property name="paramType" value="numerical"/>
            <property name="paramValue" value="(0-max)"/>
            <property name="description" value="return one's yearly income"/>
            <property name="returnType" value="Double"/>
        </Variable>

        <Variable name="RepaymentFrequencyVariable" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="repaymentFrequency"/>
            <property name="paramType" value="numerical"/>
            <property name="paramValue" value="(0-max)"/>
            <property name="description" value="return one's average repayment frequency"/>
            <property name="returnType" value="Double"/>
        </Variable>

        <Variable name="AgeVariable" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="age"/>
            <property name="paramType" value="numerical"/>
            <property name="paramValue" value="(0-max)"/>
            <property name="description" value="return one's average repayment frequency"/>
            <property name="returnType" value="Double"/>
        </Variable>
    </Variables>
</VariableConfiguration>