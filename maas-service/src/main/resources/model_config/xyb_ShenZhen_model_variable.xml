<VariableConfiguration>
    <Model>XYBModel-ShenZhen</Model>
    <Variables>
        <bean id="httpHandler" class="com.ecreditpal.maas.service.model.handler.HttpV2Handler" />
        <!--学历-->
        <Variable name="学历" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="degree"/>
            <property name="paramType" value="categorical"/>
            <property name="description" value="return one's degree"/>
            <property name="returnType" value="java.lang.String"/>
        </Variable>

        <Variable name="性别" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="gender"/>
            <property name="paramType" value="categorical"/>
            <property name="description" value="return one's gender"/>
            <property name="returnType" value="java.lang.String"/>
        </Variable>

        <Variable name="婚姻状况" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="marriageStatus"/>
            <property name="paramType" value="categorical"/>
            <property name="description" value="return one's marriage status"/>
            <property name="returnType" value="String"/>
        </Variable>

        <Variable name="age" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="age"/>
            <property name="paramType" value="numerical"/>
            <property name="paramValue" value="(0-max)"/>
            <property name="description" value="return one's age"/>
            <property name="returnType" value="java.lang.Integer"/>
        </Variable>

        <!--手机入网时间-->
        <Variable name="手机入网时间" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="cellPhoneAccessTime"/>
            <property name="paramType" value="categorical"/>
            <property name="description" value="return one's cellPhone access time"/>
            <property name="returnType" value="java.lang.String"/>
        </Variable>

        <Variable name="芝麻信用" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="zhiMaCredit"/>
            <property name="paramType" value="numerical"/>
            <property name="paramValue" value="(0-max)"/>
            <property name="description" value="return one's credit score"/>
            <property name="returnType" value="java.lang.Integer"/>
        </Variable>

        <Variable name="居住情况" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="liveCase"/>
            <property name="paramType" value="categorical"/>
            <property name="description" value="return one's live case"/>
            <property name="returnType" value="java.lang.String"/>
        </Variable>

        <Variable name="p_id_city_same_ind" class="com.ecreditpal.maas.service.model.variables.CidPhoneLocationVariable">
            <property name="key" value="pIdCitySameInd"/>
            <property name="paramType" value="numerical"/>
            <property name="description" value="compare the location of cid and mobile"/>
            <property name="requestHandler" ref="httpHandler"/>
            <property name="returnType" value="java.lang.Integer"/>
        </Variable>


        <Variable name="公司性质" class="com.ecreditpal.maas.service.model.variables.SectionVariable">
            <property name="key" value="companyType"/>
            <property name="paramType" value="categorical"/>
            <property name="description" value="return company type"/>
            <property name="returnType" value="java.lang.String"/>
        </Variable>

        <Variable name="phone_gray_score" class="com.ecreditpal.maas.service.model.variables.MiguanPhoneGrayScoreVariable">
            <property name="key" value="phoneGrayScore"/>
            <property name="paramType" value="numerical"/>
            <property name="description" value="return one's phone_gray_score"/>
            <property name="requestHandler" ref="httpHandler"/>
            <property name="returnType" value="java.lang.String"/>
        </Variable>
    </Variables>
</VariableConfiguration>